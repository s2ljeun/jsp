<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>front</servlet-name>
		<servlet-class>FrontAppServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>front</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>member</servlet-name>
		<servlet-class>member.MemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>member</servlet-name>
		<url-pattern>*.mem</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>board</servlet-name>
		<servlet-class>board.BoardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>board</servlet-name>
		<url-pattern>*.board</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>shop</servlet-name>
		<servlet-class>shop.ShopServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>shop</servlet-name>
		<url-pattern>*.mall</url-pattern>
	</servlet-mapping>

	
</web-app>